import time
from jpype import *


class SendIIS:

    def __init__(self, func):
        self.func = func
        self.last_send_time = 0

    def __call__(self, msg_label):
        self.func(msg_label)

        current_send_time = time.time()
        if current_send_time - self.last_send_time > 10:
            self.last_send_time = current_send_time
