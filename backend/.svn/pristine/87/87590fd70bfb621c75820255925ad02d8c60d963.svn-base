[uwsgi]
;socket=127.0.0.1:8000
;http=127.0.0.1:8000
socket = uwsgi.sock
chdir=./
static-map = /static=/security_platform/static
wsgi-file=security_platform/wsgi.py
vacuum = true
processes=1
threads=2
master=True
pidfile=logs/uwsgi.pid
log-maxsize = 5000000
daemonize=logs/uwsgi.log
log-format = UWSGI: %(ftime) %(user) %(addr) %(method) %(uri) %(status) %(size) %(msecs) %(uagent)
chmod-socket = 666
virtualenv=/home/admin/.virtualenvs/sms
buffer-size=65535
post-buffering=32768
listen=128

;disable-logging = true
;py-autoreload = 1

[MYSQL]
MYSQL_HOST = 127.0.0.1
MYSQL_PORT = 3306
MYSQL_USER = security_platform
MYSQL_PASSWORD = zhengxin123
MYSQL_NAME = security_platform_v2

[REDIS]
REDIS_HOST = 127.0.0.1:6379

[MQTT]
;MQTT_SERVER_IP = 127.0.0.1
MQTT_SERVER_IP = 192.168.21.150
MQTT_PORT = 1883
MQTT_USER = admin
MQTT_PWD = admin
# DEBUG会把mq收发消息的日志写入文件
LOG_LEVEL = INFO

[CAMERA]
