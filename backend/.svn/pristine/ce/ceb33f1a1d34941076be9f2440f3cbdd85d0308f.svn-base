[uwsgi]
socket=127.0.0.1:8010
;http=127.0.0.1:8000
;socket = uwsgi.sock
chdir=./
static-map = /static=/security_platform/static
wsgi-file=security_platform/wsgi.py
vacuum = true
processes=1
threads=2
master=True
pidfile=logs/uwsgi.pid
log-maxsize = 5000000
daemonize=logs/uwsgi.log
log-format = UWSGI: %(ftime) %(user) %(addr) %(method) %(uri) %(status) %(size) %(msecs) %(uagent)
chmod-socket = 666
virtualenv=/Users/lwl/.virtualenvs/SMS
buffer-size=65535
post-buffering=32768
listen=128
disable-logging = true
;py-autoreload = 1

[WEBSOCKET]
WORKERS = 4
PORT = 8001
WEBSOCKET_ADDRESS = ws://127.0.0.1/ws/

[MYSQL]
MYSQL_HOST = 127.0.0.1
MYSQL_PORT = 3306
MYSQL_USER = sms
MYSQL_PASSWORD = GyjcAb1@!#
MYSQL_NAME = security_platform_v2

[REDIS]
REDIS_HOST = 127.0.0.1:6379

[MQTT]
MQTT_SERVER_IP = 192.168.20.99
;MQTT_SERVER_IP = 192.168.21.150
